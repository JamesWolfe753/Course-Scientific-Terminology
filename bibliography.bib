  volume = {{41}},
  number = {7},
  journaltitle = {{Journal of Pediatric Surgery}},
  shortjournal = {{Journal of Pediatric Surgery}},
  author = {Soutis, Michael},
  urldate = {2014-04-10},
  date = {2006-07},
  year = {2006},
  pages = {1302--1308},
  file = {Soutis - 2006 - Ancient Greek terminology in pediatric surgery Ab.pdf:/Users/Andrew/Library/Application Support/Zotero/Profiles/muvdg23m.default/zotero/storage/D3S53Q2A/Soutis - 2006 - Ancient Greek terminology in pediatric surgery Ab.pdf:application/pdf}
