  langid = {english},
  note = {PMID: 22461143},
  keywords = {Anatomy,English,Etymology,History, 15th Century,History, 16th Century,History, 17th Century,History, 18th Century,History, 19th Century,History, 20th Century,History, 21st Century,History, Ancient,History, Medieval,Humans,Language,Linguistics,Terminology,Terminology as Topic},
