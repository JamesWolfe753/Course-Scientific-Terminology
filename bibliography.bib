@article{pampush:2011familiarity,
  abstract = {Commonly used technical anatomy and physiology (A\&P) terms are predominantly rooted in Latin and Greek vocabulary, so it is commonly inferred that a solid grounding in Latin and Greek roots of medical terminology will improve student learning in anatomy and related disciplines. This study examines the association of etymological knowledge of A\&P terms and A\&P course performance among 446 undergraduates in their first semester of the study of human gross anatomy and physiology, with a more detailed analysis of the characteristics of 52 students who filled out surveys about their prior knowledge and experiences related to medicine or anatomy. In both data sets, there was only a weak positive correlation between the performance on a quiz of Latin and Greek medical terms and the students' performance on regular assignments. The presumption that familiarity with Latin and Greek word roots has a strong influence on successful learning of anatomy is not supported by these data. Future research should address whether or not there are particular skills associated with using etymological knowledge in improving A\&P course performance. Anat Sci Educ. © 2010 American Association of Anatomists.},
  doi = {10.1002/ase.189},
  issn = {1935-9780},
  rights = {{Copyright © 2010 American Association of Anatomists}},
  title = {{Familiarity with Latin and Greek anatomical terms and course performance in undergraduates}},
  url = {http://onlinelibrary.wiley.com/doi/10.1002/ase.189/abstract},
  volume = {{4}},
  number = {1},
  journaltitle = {{Anatomical Sciences Education}},
  shortjournal = {{Anat Sci Ed}},
  author = {Pampush, James D. and Petto, Andrew J.},
  urldate = {2014-04-10},
  date = {2011},
  year = {2011},
  pages = {9--15},
  langid = {english},
  note = {PMID: 21265031},
  keywords = {Anatomy,Confounding Factors (Epidemiology),Education, Premedical,Educational Measurement,Female,Humans,Language,Latin and Greek vocabulary,Male,Students,Terminology as Topic,anatomy and physiology course,anatomy education,medical terminology,student assessment,undergraduate education},
  file = {Pampush and Petto - 2011 - Familiarity with Latin and Greek anatomical terms .pdf:/Users/Andrew/Library/Application Support/Zotero/Profiles/muvdg23m.default/zotero/storage/J5UPUGQN/Pampush and Petto - 2011 - Familiarity with Latin and Greek anatomical terms .pdf:application/pdf}
}

@article{sarma:2004etymology,
  abstract = {Recognition of word roots and the pattern of evolution of scientific terms can be helpful in understanding chemistry concepts (gaining knowledge of new concepts represented by related terms). The meaning and significance of various etymological roots, occurring as prefixes and suffixes in technical terms particularly of organic chemistry, are explained in a unified manner in order to show the connection of various concepts vis à vis the terms in currency. The meanings of some special words and many examples are provided. The interesting aspects of history and culture often involved in the evolution of terms will help sustain an abiding engagement in the study of chemistry.},
  doi = {10.1021/ed081p1437},
